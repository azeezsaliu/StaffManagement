@model IEnumerable<Staff>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Gender</th>
            <th>Age</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Staff s in Model)
        {
            <tr>
                <td>@s.Id</td>
                <td>@s.FirstName</td>
                <td>@s.LastName</td>
                <td>@s.Gender</td>
                <td>@s.Age</td>
                <td>
                    <a class="btn btn-info btn-sm" href="/Staff/Detail/@s.Id">Detail</a> |
                    <a class="btn btn-success btn-sm" asp-controller="Staff" asp-action="Edit" asp-route-id="@s.Id">Edit</a> |
                    <a class="btn btn-danger btn-sm" href="#">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
